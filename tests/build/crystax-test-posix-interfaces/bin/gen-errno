gen()
{
    local name=$1
    if [ -z "$name" ]; then
        echo "ERROR: empy name passed to 'gen' function" 1>&2
        exit 1
    fi
    echo "#ifndef ${name}"
    echo "#error ${name} not defined"
    echo "#endif"
}

gen E2BIG
gen EACCES
gen EADDRINUSE
gen EADDRNOTAVAIL
gen EAFNOSUPPORT
gen EAGAIN
gen EALREADY
gen EBADF
gen EBADMSG
gen EBUSY
gen ECANCELED
gen ECHILD
gen ECONNABORTED
gen ECONNREFUSED
gen ECONNRESET
gen EDEADLK
gen EDESTADDRREQ
gen EDOM
gen EDQUOT
gen EEXIST
gen EFAULT
gen EFBIG
gen EHOSTUNREACH
